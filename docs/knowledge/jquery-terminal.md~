/scripts/update-contributors-readme:
--------------------------------------------------------------------------------
 1 | #!/bin/bash
 2 | 
 3 | repo=$1
 4 | input=$2
 5 | marker=$3
 6 | file=$4
 7 | 
 8 | 
 9 | CONTRIBUTORS=`./scripts/contributors.mjs -r $repo -f $input -m | tr '\n' 'â˜º'`
10 | if echo "$CONTRIBUTORS" | grep ERROR > /dev/null; then
11 |     echo "$CONTRIBUTORS" | tr 'â˜º' '\n'
12 | else
13 |     cat $file | sed -n "/${marker}-START/{p;:a;N;/${marker}-END/!ba;s%.*\n%${CONTRIBUTORS}%};p" | tr 'â˜º' '\n' > README.tmp
14 |     cp README.tmp $file
15 |     rm README.tmp
16 | fi
