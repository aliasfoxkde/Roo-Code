 1 | /**
 2 |  * Copyright (c) 2019 The xterm.js authors. All rights reserved.
 3 |  * @license MIT
 4 |  */
 5 | 
 6 | const path = require('path');
 7 | 
 8 | const addonName = 'UnicodeGraphemesAddon';
 9 | const mainFile = 'addon-unicode-graphemes.js';
10 | 
11 | module.exports = {
12 |   entry: `./out/${addonName}.js`,
13 |   devtool: 'source-map',
14 |   module: {
15 |     rules: [
16 |       {
17 |         test: /\.js$/,
18 |         use: ["source-map-loader"],
19 |         enforce: "pre",
20 |         exclude: /node_modules/
21 |       }
22 |     ]
23 |   },
24 |   resolve: {
25 |     modules: ['./node_modules'],
26 |     extensions: [ '.js' ],
27 |     alias: {
28 |       common: path.resolve('../../out/common'),
29 |       vs: path.resolve('../../out/vs')
30 |     }
31 |   },
32 |   output: {
33 |     filename: mainFile,
34 |     path: path.resolve('./lib'),
35 |     library: addonName,
36 |     libraryTarget: 'umd',
37 |     // Force usage of globalThis instead of global / self. (This is cross-env compatible)
38 |     globalObject: 'globalThis',
39 |   },
40 |   mode: 'production'
41 | };
42 | 


