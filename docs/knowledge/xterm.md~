/src/common/TypedArrayUtils.test.ts:
--------------------------------------------------------------------------------
 1 | /**
 2 |  * Copyright (c) 2018 The xterm.js authors. All rights reserved.
 3 |  * @license MIT
 4 |  */
 5 | import { assert } from 'chai';
 6 | import { concat } from 'common/TypedArrayUtils';
 7 | 
 8 | type TypedArray = Uint8Array | Uint16Array | Uint32Array | Uint8ClampedArray | Int8Array | Int16Array | Int32Array | Float32Array | Float64Array;
 9 | 
10 | function deepEquals(a: TypedArray, b: TypedArray): void {
11 |   assert.equal(a.length, b.length);
12 |   for (let i = 0; i < a.length; ++i) {
13 |     assert.equal(a[i], b[i]);
14 |   }
15 | }
16 | 
17 | describe('typed array convenience functions', () => {
18 |   it('concat', () => {
19 |     const a = new Uint8Array([1, 2, 3, 4, 5]);
20 |     const b = new Uint8Array([6, 7, 8, 9, 0]);
21 |     const merged = concat(a, b);
22 |     deepEquals(merged, new Uint8Array([1, 2, 3, 4, 5, 6, 7, 8, 9, 0]));
23 |   });
24 | });
25 | 


--------------------------------------------------------------------------------
/src/common/TypedArrayUtils.ts:
--------------------------------------------------------------------------------
 1 | /**
 2 |  * Copyright (c) 2018 The xterm.js authors. All rights reserved.
 3 |  * @license MIT
 4 |  */
 5 | 
 6 | export type TypedArray = Uint8Array | Uint16Array | Uint32Array | Uint8ClampedArray | Int8Array | Int16Array | Int32Array | Float32Array | Float64Array;
 7 | 
 8 | /**
 9 |  * Concat two typed arrays `a` and `b`.
10 |  * Returns a new typed array.
11 |  */
12 | export function concat<T extends TypedArray>(a: T, b: T): T {
13 |   const result = new (a.constructor as any)(a.length + b.length);
14 |   result.set(a);
15 |   result.set(b, a.length);
16 |   return result;
17 | }
18 | 


