 1 | /**
 2 |  * Copyright (c) 2019 The xterm.js authors. All rights reserved.
 3 |  * @license MIT
 4 |  */
 5 | 
 6 | const path = require('path');
 7 | 
 8 | const addonName = 'AttachAddon';
 9 | const mainFile = 'addon-attach.js';
10 | 
11 | module.exports = {
12 |   entry: `./out/${addonName}.js`,
13 |   devtool: 'source-map',
14 |   module: {
15 |     rules: [
16 |       {
17 |         test: /\.js$/,
18 |         use: ["source-map-loader"],
19 |         enforce: "pre",
20 |         exclude: /node_modules/
21 |       }
22 |     ]
23 |   },
24 |   output: {
25 |     filename: mainFile,
26 |     path: path.resolve('./lib'),
27 |     library: addonName,
28 |     libraryTarget: 'umd',
29 |     // Force usage of globalThis instead of global / self. (This is cross-env compatible)
30 |     globalObject: 'globalThis',
31 |   },
32 |   mode: 'production'
33 | };
