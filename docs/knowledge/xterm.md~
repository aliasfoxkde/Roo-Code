 4 |  */
 5 | 
 6 | const path = require('path');
 7 | 
 8 | const addonName = 'ProgressAddon';
 9 | const mainFile = 'addon-progress.js';
10 | 
11 | module.exports = {
12 |   entry: `./out/${addonName}.js`,
13 |   devtool: 'source-map',
14 |   module: {
15 |     rules: [
16 |       {
17 |         test: /\.js$/,
18 |         use: ["source-map-loader"],
19 |         enforce: "pre",
20 |         exclude: /node_modules/
21 |       }
22 |     ]
23 |   },
24 |   resolve: {
25 |     modules: ['./node_modules'],
26 |     extensions: [ '.js' ],
27 |     alias: {
28 |       common: path.resolve('../../out/common'),
29 |       browser: path.resolve('../../out/browser'),
30 |       vs: path.resolve('../../out/vs')
31 |     }
32 |   },
33 |   output: {
34 |     filename: mainFile,
35 |     path: path.resolve('./lib'),
36 |     library: addonName,
37 |     libraryTarget: 'umd',
38 |     // Force usage of globalThis instead of global / self. (This is cross-env compatible)
39 |     globalObject: 'globalThis',
40 |   },
41 |   mode: 'production'
42 | };
43 | 


