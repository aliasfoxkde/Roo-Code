/addons/addon-image/fixture/endless.sh:
--------------------------------------------------------------------------------
 1 | #!/bin/bash
 2 | 
 3 | # sixel endless mode
 4 | # Should print an endless sine curve, abort with Ctrl-C.
 5 | 
 6 | period=200
 7 | amplitude=50
 8 | 
 9 | sixels=(@\$ A\$ C\$ G\$ O\$ _\$-)
10 | pi=$(echo "scale=10; 4*a(1)" | bc -l)
11 | run=true
12 | trap run=false INT
13 | 
14 | echo -ne "\x1bP0;0;0q\"1;1#1;2;100;0;0#1"
15 | y=0
16 | while $run
17 | do
18 |   x=$(echo "s(2*${pi}*${y}/${period})*${amplitude}+2*${amplitude}+0.5" | bc -l)
19 |   echo -ne "!${x%%.*}?${sixels[$((y%6))]}"
20 |   (( y++ ))
21 | done
22 | echo -e "\x1b\\"
23 | 


