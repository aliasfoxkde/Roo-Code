 1 | use std::fs;
 2 | use pyo3::prelude::*;
 3 |
 4 |
 5 | #[pyfunction]
 6 | fn get_file_length(data: &str) ->  PyResult<u64> {
 7 |     let metadata = fs::metadata(data)?;
 8 |     Ok(metadata.len())
 9 | }
10 |
11 |
12 |
13 | #[pymodule]
14 | fn rust_abi_test(m: &Bound<'_, PyModule>) -> pyo3::PyResult<()> {
15 |     m.add_function(wrap_pyfunction!(get_file_length, m)?)?;
16 |     Ok(())
17 | }
