 6 | @run_in_pyodide(packages=["test-sharedlib-py"])
 7 | def test_sharedlib(selenium):
 8 |     from sharedlib_test import do_the_thing
 9 |
10 |     assert do_the_thing(4, 5) == 29
11 |


