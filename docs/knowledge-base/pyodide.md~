16 |     fpcast_test.varargs1()
17 |     fpcast_test.varargs2()
18 |     fpcast_test.varargs3()
19 |
20 |     fpcast_test.kwargs0()
21 |     fpcast_test.kwargs1()
22 |     fpcast_test.kwargs2()
23 |     fpcast_test.kwargs3()
24 |
